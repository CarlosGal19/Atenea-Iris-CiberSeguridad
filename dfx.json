{
    "canisters": {
        "internet_identity": {
            "type": "custom",
            "candid": "https://github.com/dfinity/internet-identity/releases/download/release-2024-10-01/internet_identity.did",
            "wasm": "https://github.com/dfinity/internet-identity/releases/download/release-2024-10-01/internet_identity_dev.wasm.gz",
            "frontend": {},
            "remote": {
                "id": {
                    "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
                }
            }
        },
        "backend": {
            "type": "motoko",
            "main": "src/backend/main.mo",
            "declarations": {
                "node_compatibility": false,
                "output": "src/declarations/backend"
            }
        },
        "react-dashboard":{
            "type": "assets",
            "dependencies": ["backend", "internet_identity"],
            "source": ["src/react-dashboard/dist"],
            "workspace": "src/react-dashboard"
        },
        "network": {
            "candid": "src/network/network.did",
            "package": "network",
            "type": "rust",
            "declarations": {
                "node_compatibility": false,
                "output": "src/declarations/network"
            }
          },
        "nlp": {
            "candid": "src/nlp/nlp.did",
            "package": "nlp",
            "type": "rust",
            "declarations": {
                "node_compatibility": false,
                "output": "src/declarations/nlp"
            }
          }
    }
}
